  "displayName": "opencode",
  "name": "opencode",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "customLiteral": "var(--syntax-function)",
    "newOperator": "var(--syntax-operator)",
    "numberLiteral": "var(--syntax-number)",
    "stringLiteral": "var(--syntax-string)"
  },
